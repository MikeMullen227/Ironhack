<h1>Add a New Concert</h1>

<%= form_for @concert do |f| %>
	<%= f.label :artist %>
	<%= f.text_field :artist %>
	
	<%= f.label :city %>
	<%= f.text_field :city%>

	<%= f.label :venue %>
	<%= f.text_field :venue%>

	<%= f.label :date %>
	<%= f.date_field :date%>

	<%= f.label :price %>
	<%= f.text_field :price%>

	<%= f.label :description%>
	<%= f.text_field :description%>


	<%= f.submit %>

<% end %>